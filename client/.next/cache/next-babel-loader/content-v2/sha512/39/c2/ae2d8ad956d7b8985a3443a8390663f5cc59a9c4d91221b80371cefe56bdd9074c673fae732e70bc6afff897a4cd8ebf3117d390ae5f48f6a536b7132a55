      alert(\r\n          `Failed to load web3, accounts, or contract. Check console for details.`,\r\n        );\r\n        console.error(error);\r\n      }\r\n    }\r\n      setup();\r\n      // console.log('ok')\r\n  },[web3]);\r\n\r\n  useEffect(()=> {//display available votes addresses\r\n    var displayVotes = async () => {\r\n      if(contract == ''){\r\n        return;\r\n      }\r\n      const response = await contract.methods.getDeployedVotes().call();\r\n      // Update state with the result.\r\n      setVotesAddresses(response);\r\n    };\r\n    displayVotes();\r\n  },[contract]);\r\n\r\n  var displayInfo = async (address) => { \r\n    try {// Get the contract instance.\r\n      const instance = new web3.eth.Contract(\r\n        VoteContract.abi,\r\n        address\r\n      );\r\n      console.log(await instance.methods.typeOfVote().call())\r\n      if((await instance.methods.typeOfVote().call()) == 0) {\r\n        return [await instance.methods.currentElection().call(), await instance.methods.getElectionVoter(currentUser).call()];\r\n      }\r\n      else{\r\n        return [await instance.methods.currentPetition().call(), await instance.methods.getElectionVoter(currentUser).call()]\r\n      }\r\n    } catch (error) {\r\n      console.error(error);\r\n    }\r\n  };\r\n\r\n  useEffect(()=> {//render votes\r\n    var renderVotes = async () => {\r\n        if (!votesAddresses || !currentUser){\r\n          return;\r\n        }\r\n        var tempElections  = [];\r\n        var tempVoted = [];\r\n        var tempPetitions = [];\r\n        await votesAddresses.forEach(address => {\r\n            displayInfo(address).then(voteInfo => {\r\n              if(voteInfo[1]){\r\n                tempElections.push(voteInfo[0]);\r\n                tempVoted.push(voteInfo[1]);\r\n              }\r\n              else{\r\n                tempPetitions.push(voteInfo[0]);\r\n                tempVoted.push(voteInfo[1]);\r\n              }\r\n          })\r\n        })\r\n        setTimeout(function(){\r\n            setElections(tempElections);\r\n            setYouVoted(tempVoted);\r\n            setPetitions(tempPetitions);\r\n          }, 1000);\r\n\r\n      }\r\n    renderVotes();\r\n  },[votesAddresses]);\r\n\r\n  // useEffect(()=> {\r\n  //   if(renderedAddresses) {\r\n  //     console.log(renderedAddresses);\r\n  //     console.log(youVoted);\r\n  //   }\r\n  // },[renderedAddresses])\r\n\r\n  function displayElectionsList() {\r\n    if(web3 == \"\"){\r\n      return \"waiting for votes to display...\"\r\n    }\r\n    else if(votesAddresses == \"\"){\r\n      return \"no votes to display\"\r\n    }\r\n\r\n    return elections ? elections.map((vote, index) => \r\n          <Link className=\"ui button\" route ={`/elections/vote/${votesAddresses[index]}`} key={index}>\r\n            <div className=\"ui card\" style={styles.card}>\r\n              <div className=\"card\">\r\n                <span className=\"right floated\">\r\n                  {vote.numVotes}\r\n                  <i className=\"user icon\" style={{margin: 3}}></i>\r\n                  {youVoted[index] ? <i className=\"check circle icon\" style={{margin: 3}}></i>  : <i className=\"circle outline icon\" style={{margin: 3}}></i>}\r\n                  \r\n                </span>\r\n\r\n                <div className=\"content\">\r\n                  <div className=\"header\" style={styles.title}>\r\n                      {vote.title} - \r\n                      { new Date() < new Date(vote.startDate * 1) ? \"starts on \" + new Date(vote.startDate * 1).toUTCString().slice(0,17) : \r\n                      (new Date() >= new Date(vote.startDate * 1) && new Date() <= new Date(vote.endDate * 1) ? \"ends on \" + new Date(vote.endDate * 1).toUTCString().slice(0,17):\r\n                       \"archived: \" + new Date(vote.endDate * 1).toUTCString().slice(0,17)\r\n                      )\r\n                      }\r\n                  </div>\r\n                </div>\r\n                <div className=\"content\">\r\n\r\n                  <span className=\"right floated\">\r\n                    <span style={styles.access} >access vote</span>\r\n                    <i className=\"angle right icon huge\"></i>\r\n                  </span>\r\n\r\n                  <div className=\"ui sub header\" style={{marginLeft:10}}>\r\n                    <i className=\"checkmark icon small\"></i>  \r\n                    election \r\n                  </div>\r\n                  <div className=\"ui feed\" style={{marginLeft:10}}>{ vote.description }</div>\r\n                </div>\r\n\r\n              </div>\r\n            </div>\r\n          </Link>\r\n      ) : <div> no votes found </div>\r\n    }\r\n\r\n    function displayPetitionsList() {\r\n      if(web3 == \"\"){\r\n        return \"waiting for votes to display...\"\r\n      }\r\n      else if(votesAddresses == \"\"){\r\n        return \"no votes to display\"\r\n      }\r\n  \r\n      return petitions ? petitions.map((vote, index) => \r\n            <Link className=\"ui button\" route ={`/petitions/vote/${votesAddresses[index]}`} key={index}>\r\n              <div className=\"ui card\" style={styles.card}>\r\n                <div className=\"card\">\r\n                  <span className=\"right floated\">\r\n                    {vote.numVotes}\r\n                    <i className=\"user icon\" style={{margin: 3}}></i>\r\n                    {youVoted[index] ? <i className=\"check circle icon\" style={{margin: 3}}></i>  : <i className=\"circle outline icon\" style={{margin: 3}}></i>}\r\n                    \r\n                  </span>\r\n  \r\n                  <div className=\"content\">\r\n                    <div className=\"header\" style={styles.title}>\r\n                        {vote.title} - \r\n                        { new Date() < new Date(vote.startDate * 1) ? \"starts on \" + new Date(vote.startDate * 1).toUTCString().slice(0,17) : \r\n                        (new Date() >= new Date(vote.startDate * 1) && new Date() <= new Date(vote.endDate * 1) ? \"ends on \" + new Date(vote.endDate * 1).toUTCString().slice(0,17):\r\n                         \"archived: \" + new Date(vote.endDate * 1).toUTCString().slice(0,17)\r\n                        )\r\n                        }\r\n                    </div>\r\n                  </div>\r\n                  <div className=\"content\">\r\n  \r\n                    <span className=\"right floated\">\r\n                      <span style={styles.access} >access vote</span>\r\n                      <i className=\"angle right icon huge\"></i>\r\n                    </span>\r\n  \r\n                    <div className=\"ui sub header\" style={{marginLeft:10}}>\r\n                      <i className=\"checkmark icon small\"></i>  \r\n                      petition\r\n                    </div>\r\n                    <div className=\"ui feed\" style={{marginLeft:10}}>{ vote.description }</div>\r\n                  </div>\r\n  \r\n                </div>\r\n              </div>\r\n            </Link>\r\n        ) : <div> no votes found </div>\r\n      }\r\n\r\n  return( \r\n    <>\r\n      <Header></Header>\r\n      <br></br>\r\n      <br></br>\r\n      <br></br>\r\n      <br></br>\r\n      <div className=\"App\">\r\n        <div>{ displayElectionsList() }</div>\r\n        <div>{ displayPetitionsList() }</div>\r\n      </div>\r\n    </>\r\n  );\r\n}\r\n\r\nexport default App;\r\n"]},"metadata":{},"sourceType":"module"}